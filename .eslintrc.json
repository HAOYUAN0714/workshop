{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint",
        "react"
    ],
    "rules": {
        // "prettier/prettier": "warn"
        "indent": [
            "error",
            4
        ],
        "no-tabs": 0,
        "no-undef": "off"
    }
}